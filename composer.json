{
  "name": "proho/laravel-batch",
  "description": "Insert and update batch (bulk) in laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Mohammad Ghanbari",
      "email": "mavin.developer@gmail.com"
    }
  ],
  "require": {
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3@dev"
  },
  "autoload": {
    "psr-4": {
      "Proho\\Batch\\": "src/"
    },
    "files": [
      "src/Common/Helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Proho\\Batch\\BatchServiceProvider"
      ],
      "aliases": {
        "Batch": "Proho\\Batch\\BatchFacade"
      }
    }
  },
  "minimum-stability": "dev"
}
